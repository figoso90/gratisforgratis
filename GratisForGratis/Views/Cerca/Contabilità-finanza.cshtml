@model GratisForGratis.Models.ListaServizi

@section Styles {
    @Styles.Render("~/Content/home.css")
    @Styles.Render("~/Content/ricerca.css")
}
<aside id="ricerca">
    @Html.Partial("Filtri/Servizi", new GratisForGratis.Models.RicercaServizioViewModel(Model.TotalNumber))
</aside>
<section id="page">
    <section id="home" class="content">
        @Html.Partial("Elenco/Servizi", Model)
    </section>
</section>

@section Scripts {
    @Scripts.Render("~/Scripts/ricerca.js")
}