@model GratisForGratis.Models.RicercaModelloViewModel

@Html.Partial("PartialPages/_FiltroOggetto", Model)

<div class="form-group col-md-2 modello">
    @Html.LabelFor(m => m.cerca_modello, new { @class = "control-label" })
    @Html.TextBoxFor(m => m.cerca_modello, new { @class = "form-control", data_autocomplete_url = "/Home/FindModello", data_autocomplete_id = "cerca_modelloID", data_autocomplete_filtro_extra = "#Cerca_Marca", placeholder = Language.Model })
    @Html.HiddenFor(m => m.cerca_modelloID)
    @Html.ValidationMessageFor(m => m.cerca_modelloID, "", new { @class = "text-danger" })
</div>

<div class="form-group col-md-12 justify-content-center align-items-center">
    <a class="btn btn-primary" href="javascript:void(0);" onclick="ricercaAvanzata('FiltroOggetti');">@Language.Find</a>
</div>