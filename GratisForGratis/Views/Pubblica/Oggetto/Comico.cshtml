@model GratisForGratis.Models.PubblicaVideoViewModel

@Html.Partial("Oggetto", (Model!=null)?Model:new GratisForGratis.Models.PubblicaOggettoViewModel())
@Html.Hidden("ActionCategoria", "/Pubblica/Video")
<div class="form-group">
    @Html.LabelFor(m => m.Formato, new { @class = "col-md-2 control-label" })
    <div class="field">
        @Html.TextBoxFor(m => m.Formato, new { @class = "form-control", data_autocomplete_url = "/Home/FindFormato", data_autocomplete_id = "FormatoID" })
        @Html.Hidden("FormatoID")
    </div>
    @Html.ValidationMessageFor(m => m.Formato, "", new { @class = "text-danger" })
</div>

<div class="form-group">
    @Html.LabelFor(m => m.Regista, new { @class = "col-md-2 control-label" })
    <div class="field">
        @Html.TextBoxFor(m => m.Regista, new { @class = "form-control", data_autocomplete_url = "/Home/FindRegista", data_autocomplete_id = "RegistaID" })
        @Html.Hidden("RegistaID")
    </div>
    @Html.ValidationMessageFor(m => m.Regista, "", new { @class = "text-danger" })
</div>